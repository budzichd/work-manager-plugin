{
    "manifest_version": 3,
    "name": "Work Manager",
    "description": "Extension that help you with improving your work system",
    "version": "1.0",
    "side_panel": {
      "default_path": "index.html",
      "openPanelOnActionClick": true
    },
    "background": {
      "service_worker": "service-worker.js"
    },
    "action": {
      "default_title": "Click to open panel"
    },
    "permissions": [
      "sidePanel"
    ],
    "icons": {
      "16": "base-icon.png"
    }
}